def solution(num, k):
    num_l = [int(c) for c in str(num)]
# 위에거가 문자열, 숫자열을 각각 자리에 맞게 리스트로 바꾸는 명령어
    if k in num_l:
        a = num_l.index(k)
    else: 
        return -1
    return a+1


def solution(num, k):
    return -1 if str(k) not in str(num) else str(num).find(str(k)) + 1



def solution(num, k):
