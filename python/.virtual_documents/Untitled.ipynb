def solution(my_string):
    answer = ''
    dic = {}
    for m in my_string:
        if m not in dic:
            dic[m] = 1
            answer += m
        elif m in dic:
            continue

    return answer


def solution(my_string):
    answer = ''
    for i in range(len(my_string)):
        if my_string[i] not in answer:
            answer += my_string[i]
    return answer



